{
    "Button component": {
        "prefix": ["bc"],
        "body": [
            "<script lang=\"ts\">",
            "    import { createEventDispatcher } from \"svelte\";",
            "    const dispatch = createEventDispatcher();",
            "    function click() {",
            "        dispatch(\"click\");",
            "    }",
            "</script>",
            "<button on:click={click}>",
            "    <div>",
            "    </div>",
            "</button>"
        ]
    },
    "svelte body": {
        "prefix": ["s"],
        "body": ["<script lang=\"ts\">", "</script>", "<div>", "</div>"]
    },
    "Steeze ui hero icon": {
        "prefix": ["hi"],
        "body": [
            "import { Icon } from \"@steeze-ui/svelte-icon\";",
            "import { ${1:IconName} } from \"@steeze-ui/heroicons\";",
            "<Icon src={ ${1:IconName} } class=\"${2:InsertClassHere}\" theme=\"outline\" />"
        ]
    },
    "transparent border": {
        "prefix": ["bt"],
        "body": ["border border-transparent"]
    },
    "flex-row": {
        "prefix": ["fr"],
        "body": ["flex flex-row"]
    },
    "svelte component import": {
        "prefix": ["sci"],
        "body": [
            "import ${1:ComponentName} from \"\\$lib/${2:path/to/component}/${1:ComponentName}.svelte\";"
        ]
    },
    "i18n": {
        "prefix": ["i18n"],
        "body": ["import { _ } from \"svelte-i18n\";"]
    },
    "script tag": {
        "prefix": ["script"],
        "body": ["<script lang=\"ts\">", "</script>"]
    },
    "div": {
        "prefix": ["div"],
        "body": ["<div class=\"\">", "</div>"]
    },
    "message": {
        "prefix": ["message"],
        "body": ["\\$_(\"$1\")"]
    }
}
