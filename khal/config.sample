# This is based on the khal config in ~/.config/khal/config
[calendars]

# EDIT ME
[[$calendar]]
path = ~/calendars/*
type = discover

# EDIT ME
[[$contacts]]
path = ~/contacts/*
type = discover

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M
